package com.igor.cursojava.aula01;

public class OlaMundo {

	public static void main(String[] args) {
		
		System.out.println("Olá MUNDO!!! Seja-bem vinda familia 69 ao mundo JAVA");

		String nome = "Bianca Akemi";
		int idade = 25;
		double altura = 1.58;
		
		System.out.println("\n\t Olá " + nome + " você tem: "+ idade +" anos " + "e mede: " + altura + " metros");
		System.out.println("\n só lembrando que eu tenho " + idade + " ano(s)...");
	
		
	}

}
